#include <unistd.h>
#include <string.h>
#include <stdio.h>
#include "main.h"
/**
 * print_array - prints n elements of an array of integers
 * @a: array to be supplied
 * @n: number of elements of array to be printed
 */

void print_array(int *a, int n)
{
int i;
i = 0;
while (i < n)
{
printf("%d, ", a[i]);
i++;
}
printf("%d\n", a[i]);
}
