#include <stdlib.h>
#include "dog.h"

/**
 * *new_dog - creates a new dog
 * @name - char * type
 * @age - float type
 * @owner - char * type
 * return - NULL if function fails
 **/

dog_t *new_dog(char *name, float age, char *owner)
{
if (d != NULL)
{
(*d).name = name;
(*d).age = age;
(*d).owner = owner;
}
}
